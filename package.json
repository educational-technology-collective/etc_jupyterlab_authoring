{
  "name": "@educational-technology-collective/etc_jupyterlab_authoring",
  "version": "0.1.12",
  "description": "A JupyterLab extension.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "git@github.com:educational-technology-collective/etc_jupyterlab_authoring.git",
  "bugs": {
    "url": "git@github.com:educational-technology-collective/etc_jupyterlab_authoring.git/issues"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "ETC",
    "email": ""
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,.js,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "schema/*.json"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "git@github.com:educational-technology-collective/etc_jupyterlab_authoring.git"
  },
  "scripts": {
    "build": "jlpm run build:lib && jlpm run build:labextension:dev",
    "build:prod": "jlpm run clean && jlpm run build:lib && jlpm run build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc",
    "clean": "jlpm run clean:lib",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:labextension": "rimraf etc_jupyterlab_authoring/labextension",
    "clean:all": "jlpm run clean:lib && jlpm run clean:labextension",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "install:extension": "jlpm run build",
    "watch": "run-p watch:src watch:labextension",
    "watch:src": "tsc -w",
    "watch:labextension": "jupyter labextension watch ."
  },
  "dependencies": {
    "@jupyterlab/application": "^3.1.0",
    "@jupyterlab/apputils": "^3.1.18",
    "@jupyterlab/codeeditor": "^3.0.7",
    "@jupyterlab/codemirror": "^3.0.2",
    "@jupyterlab/completer": "^3.0.5",
    "@jupyterlab/console": "^3.0.6",
    "@jupyterlab/coreutils": "^5.1.0",
    "@jupyterlab/launcher": "^3.0.5",
    "@jupyterlab/logconsole": "^3.0.8",
    "@jupyterlab/mainmenu": "^3.0.5",
    "@jupyterlab/notebook": "^3.0.7",
    "@jupyterlab/rendermime": "^3.0.8",
    "@jupyterlab/services": "^6.1.0",
    "@jupyterlab/settingregistry": "^3.1.0",
    "@lumino/algorithm": "^1.7.0",
    "@lumino/commands": "^1.12.0",
    "@lumino/signaling": "^1.4.3",
    "@types/codemirror": "0.0.87",
    "@types/dom-mediacapture-record": "^1.0.10"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mkdirp": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "typescript": "~4.1.3"
  },
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "styleModule": "style/index.js",
  "jupyterlab": {
    "discovery": {
      "server": {
        "managers": [
          "pip"
        ],
        "base": {
          "name": "etc_jupyterlab_authoring"
        }
      }
    },
    "extension": true,
    "outputDir": "etc_jupyterlab_authoring/labextension",
    "schemaDir": "schema"
  }
}
